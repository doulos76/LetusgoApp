project 'Letusgo.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'
inhibit_all_warnings!

target 'Letusgo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Letusgo
  pod 'RxValidator'

  pod 'Firebase/Core'
  pod 'Firebase/RemoteConfig'
  pod 'Firebase/Performance'
  pod 'Fabric'
  pod 'Crashlytics'

  target 'LetusgoTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
  end

  target 'LetusgoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
